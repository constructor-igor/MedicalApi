<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
  <appSettings>
    <add key="username" value="USER_NAME" /> <!--Here enter your API user name-->
    <add key="password" value="PASSWORD" /> <!--Here enter your API password-->
    <add key="priaid_healthservice_url" value="https://sandbox-healthservice.priaid.ch" /> <!-- Be aware that sandbox link is for testing pourpose (not real data) once you get live access you shold use https://healthservice.priaid.ch -->
    <add key="priaid_authservice_url" value="https://sandbox-authservice.priaid.ch/login" /> <!-- Be aware that sandbox link is for testing pourpose (not real data) once you get live access you shold use https://authservice.priaid.ch/login -->
    <add key="language" value="en-gb" />
  </appSettings>
</configuration>